{
  "mcpServers": {
    "taskade": {
      "command": "npx",
      "args": ["-y", "@taskade/mcp-server"],
      "env": {
        "TASKADE_API_KEY": "${TASKADE_API_KEY}"
      }
    },
    "brightdata": {
      "command": "npx",
      "args": ["@brightdata/mcp"],
      "env": {
        "API_TOKEN": "${BRIGHTDATA_API_TOKEN}"
      }
    },
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone=Europe/London"]
    },
    "graphiti-memory": {
      "transport": "sse",
      "url": "https://graphiti.mcp.finproductions.uk/sse"
    },
    "calendar": {
      "transport": "sse",
      "url": "https://n8n.finproductions.uk/mcp/my-calendar/sse",
      "headers": {
        "Authorization": "${CALENDAR_MCP_TOKEN}",
        "X-Custom-Header": "value"
      }
    }
  }
}
